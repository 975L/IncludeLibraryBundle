{% apply spaceless %}
<script
    {% if not data.defer is defined or data.defer == true %}
        defer
    {% endif %}
{# src #}
    {% if data.src is defined %}
        src="{{ data.src }}{{ params }}"
    {% endif %}
{# integrity #}
    {% if data.integrity is defined and null != data.integrity and '' != data.integrity %}
        integrity="{{ data.integrity }}"
    {% endif %}
{# crossorigin #}
    {% if data.crossorigin is defined %}
        crossorigin="{{ data.crossorigin }}"
    {% endif %}
></script>
{% endapply %}