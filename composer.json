{
    "name": "c975l/includelibrary-bundle",
    "type": "symfony-bundle",
    "description": "Bundle to include easily main libraries jQuery, Bootstrap, etc. to your html header",
    "keywords": ["symfony", "bundle", "library"],
    "homepage": "https://github.com/975L/IncludeLibraryBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Laurent Marquet",
            "email": "laurent.marquet@laposte.net",
            "homepage": "https://github.com/Laurent3170",
            "role": "Developer"
        },
        {
            "name": "975L Community",
            "homepage": "https://github.com/975L"
        }
    ],
    "require": {
        "php": "^7",
        "twig/extensions": "^1.5"
    },
    "require-dev": {
        "phpstan/phpstan": "^0",
        "phpunit/phpunit": "^7",
        "rector/rector": "^0",
        "symplify/easy-coding-standard": "^5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "c975L\\IncludeLibraryBundle\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "support": {
        "email": "contact@975l.com",
        "source": "https://github.com/975L/IncludeLibraryBundle",
        "issues": "https://github.com/975L/IncludeLibraryBundle/issues"
    }
}
