{% spaceless %}
<script
    type="text/javascript"
{# src #}
    {% if data.src != null %}
        src="{{ data.src }}"
    {% endif %}
{# integrity #}
    {% if data.integrity != null %}
        integrity="{{ data.integrity }}"
    {% endif %}
{# crossorigin #}
    {% if data.crossorigin != null %}
        crossorigin="{{ data.crossorigin }}"
    {% endif %}
></script>
{% endspaceless %}